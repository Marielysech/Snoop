  },
  "homepage": "https://github.com/Marielysech/Snoop#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "collect.js": "^4.32.0",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "~4.16.1",
    "express-session": "^1.17.2",
    "gridfs-stream": "^1.1.1",
    "http-errors": "^2.0.0",
    "moment": "^2.29.3",
